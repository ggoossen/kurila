# doio.c [Perl_do_openn]
use Config;
BEGIN {
    if (!config_value("useperlio")) {
        print: $^STDOUT, <<EOM;
SKIPPED
# warns only with perlio
EOM
        exit;
    }
}
use warnings 'io' ;
close $^STDIN;
open: my $fh1, ">", "doiowarn.tmp"; close $fh1;
no warnings 'io' ;
open: my $fh2, ">", "doiowarn.tmp"; close $fh2;
unlink: "doiowarn.tmp";
EXPECT
