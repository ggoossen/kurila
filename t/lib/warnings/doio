# doio.c [Perl_do_open9]
use warnings 'io' ;
open(my $f, '|-|', "$^EXECUTABLE_NAME -e 1");
close($f);
no warnings 'io' ;
open(my $g, '|-|', "$^EXECUTABLE_NAME -e 1");
close($g);
EXPECT
Can't open bidirectional pipe at - line 3 character 1.
