  close() on unopened filehandle %s     [Perl_do_close]
    $a = "fred";close("$a")

  tell() on closed filehandle           [Perl_do_tell]
    $a = "fred";$a = tell($a)

  seek() on closed filehandle           [Perl_do_seek]
    $a = "fred";$a = seek($a,1,1)

  sysseek() on closed filehandle        [Perl_do_sysseek]
    $a = "fred";$a = seek($a,1,1)

  warn(warn_uninit);                    [Perl_do_print]
    print $a ;

