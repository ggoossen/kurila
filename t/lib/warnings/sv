# sv.c
use warnings 'printf' ;
open: my $f, ">", ($^OS_NAME eq 'VMS' ?? 'NL:' !! '/dev/null') ;
printf: $f, "\%z\n" ;
my $a = sprintf: "\%z" ;
printf: $f, "\%" ;
$a = sprintf: "\%" ;
printf: $f, "\%\x{02}" ;
$a = sprintf: "\%\x{02}" ;
no warnings 'printf' ;
printf: $f, "\%z\n" ;
$a = sprintf: "\%z" ;
printf: $f, "\%" ;
$a = sprintf: "\%" ;
printf: $f, "\%\x{02}" ;
$a = sprintf: "\%\x{02}" ;
EXPECT
